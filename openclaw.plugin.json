{
  "id": "web_search_plus",
  "name": "Web Search Plus",
  "description": "Multi-provider web search with fallback and monthly usage limits",
  "version": "1.0.0",
  "configSchema": {
    "type": "object",
    "additionalProperties": true,
    "properties": {
      "providers": {
        "type": "array",
        "items": {
          "type": "object",
          "additionalProperties": true,
          "properties": {
            "id": { "type": "string" },
            "type": { "type": "string" },
            "apiKey": { "type": "string" },
            "monthlyLimit": { "type": "number" },
            "baseUrl": { "type": "string" }
          }
        }
      },
      "primaryProviderId": { "type": "string" }
    }
  }
}
